	"### reference for tokenColor": [
		"https://macromates.com/manual/en/language_grammars",
		"https://www.sublimetext.com/docs/scope_naming.html"
	],
	"tokenColors": [
		{
			"name": "Default",
			"scope": [
				"source",
				"### generic",
				"keyword.operator",
				"punctuation",
				"storage.type.function.arrow",
				"### powershell",
				"variable.other.readwrite.powershell",
				"### python",
				"constant.character.format.placeholder.other.python"
			],
			"settings": {
				"foreground": "#CFCFCF",
				"fontStyle": ""
			}
		},
		{
			"name": "Comments and Documentations",
			"scope": [
				"comment",
				"comment.block",
				"comment.documentation",
				"constant.string.documentation",
				"punctuation.definition.comment"
			],
			"settings": {
				"foreground": "#8F8F8F",
				"fontStyle": ""
			}
		},
		{
			"name": "Keywords",
			"scope": [
				"keyword",
				"keyword.control",
				"keyword.operator.wordlike",
				"storage.modifier",
				"storage.type.function",
				"punctuation.definition.variable",
				"### powershell",
				"storage.type.powershell",
				"### kotlin",
				"#### var val",
				"source.kotlin storage.type.kotlin"
			],
			"settings": {
				"foreground": "#FF8FFF",
				"fontStyle": ""
			}
		},
		{
			"name": "Meta and Escape components",
			"scope": [
				"constant.character.escape",
				"entity.name.type.annotation",
				"meta.declaration.annotation",
				"meta.template",
				"punctuation.definition.annotation",
				"punctuation.definition.template-expression.begin",
				"punctuation.definition.template-expression.end",
				"punctuation.section.embedded.substatement.begin",
				"punctuation.section.embedded.substatement.end",
				"keyword.operator.documentation",
				"keyword.other.documentation",
				"storage.type.annotation",
			],
			"settings": {
				"foreground": "#AFAFFF",
				"fontStyle": ""
			}
		},
		{
			"name": "Functions",
			"scope": [
				"entity.name.function",
				"support.function",
				"variable.other.member",
				"meta.member.access"
			],
			"settings": {
				"foreground": "#6FBFFF",
				"fontStyle": ""
			}
		},
		{
			"name": "Properties/Fields",
			"scope": [
				"property",
				"variable.other.property",
			],
			"settings": {
				"foreground": "#4FCFCF",
				"fontStyle": ""
			}
		},
		{
			"name": "Static Properties/Fields",
			"scope": [
				"variable.language",
			],
			"settings": {
				"foreground": "#4FCFCF",
				"fontStyle": "bold"
			}
		},
		{
			"name": "Constant Properties/Fields",
			"scope": [
				"constant",
			],
			"settings": {
				"foreground": "#4FCFCF",
				"fontStyle": "bold underline"
			}
		},
		{
			"name": "Formal Parameters / Automatic Variables",
			"scope": [
				"variable.parameter",
				"variable.other.property",
				"support.type.property-name",
				"support.variable.automatic",
				"### kotlin",
				"#### annotation's named parameter",
				"meta.declaration.annotation.kotlin constant.other.key.kotlin",
			],
			"settings": {
				"foreground": "#5FCF5F",
				"fontStyle": ""
			}
		},
		{
			"name": "Type",
			"scope": [
				"entity.name.class",
				"entity.name.type",
				"entity.other.inherited-class",
				"support.class",
				"storage.type",
				"### powershell",
				"meta.group.simple.subexpression.powershell storage.type.powershell",
				"meta.scriptblock.powershell storage.type.powershell",
				"### python",
				"meta.function.parameters.python",
				"meta.item-access.arguments.python",
				"meta.function-call.arguments.python"
			],
			"settings": {
				"foreground": "#CFBF4F",
				"fontStyle": ""
			}
		},
		{
			"name": "Type (Single instance)",
			"scope": [
				"### kotlin",
				"source.kotlin meta.class.kotlin"
			],
			"settings": {
				"foreground": "#CFBF4F",
				"fontStyle": "bold"
			}
		},
		{
			"name": "Primitive Literals",
			"scope": [
				"string",
				"constant.numeric",
				"constant.language",
				"punctuation.definition.string.begin",
				"punctuation.definition.string.end"
			],
			"settings": {
				"foreground": "#EFAF5F",
				"fontStyle": ""
			}
		},
		{
			"scope": [
				"invalid.illegal",
				"invalid.unimplemented"
			],
			"settings": {
				"foreground": "#FF9F9F",
				"fontStyle": ""
			}
		}
	]
}